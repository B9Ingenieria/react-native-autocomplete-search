{
  "name": "@b9/react-native-autocomplete-search",
  "version": "1.1.3",
  "description": "react-native component used for autocompletion when searching data from local or remote source",
  "main": "lib/module/index.js",
  "repository": "https://github.com/B9Ingenieria/react-native-autocomplete-search.git",
  "author": "B9 Ingenieria",
  "license": "MIT",
  "keywords": [
    "react-native",
    "react-component",
    "search",
    "local",
    "remote",
    "source",
    "autocomplete",
    "autocompletion",
    "autosuggestion",
    "autosuggest",
    "suggestion",
    "suggest"
  ],
  "scripts": {
    "test": "jest",
    "prepare": "yarn run snyk-protect && bob build",
    "snyk-protect": "snyk protect"
  },
  "peerDependencies": {
    "prop-types": "*",
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@react-native-community/bob": "^0.14.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.86.0",
    "jest": "^23.6.0",
    "react": "^16.6.3",
    "regenerator-runtime": "^0.13.1"
  },
  "dependencies": {
    "@babel/preset-flow": "^7.9.0",
    "lodash": "^4.17.12",
    "snyk": "^1.332.1"
  },
  "jest": {
    "verbose": true,
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/lib/"
    ]
  },
  "react-native": "src/index.js",
  "module": "lib/module/index.js",
  "files": [
    "src",
    "lib",
    "!**/__tests__"
  ],
  "@react-native-community/bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "module"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "snyk": true
}
